  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5a7a7a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#0a1f1f"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#0a5c5c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control", "constant.other.color"],
      "settings": {
        "foreground": "#0a5c5c"
      }
    },
    {
      "scope": ["entity.name.tag", "entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#0097a7"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#388e3c"
      }
    },
    {
      "scope": ["support.type", "support.class", "storage.type.cs"],
      "settings": {
        "foreground": "#0097a7"
      }
    },
    {
      "scope": ["string", "constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#388e3c"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#7b1fa2"
      }
    },
    {
      "scope": ["variable.parameter.function"],
      "settings": {
        "foreground": "#f57c00"
      }
    },
    {
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#7b1fa2"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#0097a7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#7b1fa2"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#d32f2f"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#0a5c5c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#0a5c5c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#388e3c"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#d32f2f"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#f5b800"
      }
    },
    {
      "scope": ["punctuation.definition.quote.begin.markdown"],
      "settings": {
        "foreground": "#388e3c"
      }
    },
    {
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": {
        "foreground": "#0097a7"
      }
    },
    {
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#388e3c"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#0a5c5c"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#f5b800"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#d32f2f"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#7b1fa2"
      }
    }
  ]
}
